{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/game",
      "destination": "/public/wordsearch.html"
    },
    {
      "source": "/docs",
      "destination": "/public/docs.html"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}